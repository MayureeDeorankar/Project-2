list1 = [ ]
n = int(input("Enter the  number of elements in your list  "))
print("Enter the elements in your list")
for i in range(n):
    element = int(input(" "))
    list1.append(element)
print("your list is" , list1)
positive = [ ]
for i in list1:
    if(i>0):
        positive.append(i)
print("positive numbers in your list are" , positive)
    
